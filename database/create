CREATE DATABASE pinger;
USE pinger;
CREATE TABLE users(
    id INT AUTO_INCREMENT PRIMARY KEY NOT NULL,
    pseudo VARCHAR(255),
    mail TEXT,
    password TEXT,
    role INT,
    date TIMESTAMP,
    archived INT
);
